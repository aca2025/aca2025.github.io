---
layout: default
---

{{ page.content }}

<br><br>

{% assign sorted_sessions = site.sessions | sort: 'AA' %}

{% assign sorted_participants = site.data.participants  %}


{% for session in sorted_sessions  %}   

  <h2> {{ session.title }} </h2>

  <table class="table table-striped">
    <tbody>
      {% for talk in site.data.talks %}
         {% if talk.session ==  session.AA  %}
            <tr>
              <td style="text-align: left"><strong> {{ talk.title }} </td>
            </tr>
        {% end if %}    
      {% endfor %}
    </tbody>
  </table>
{% endfor %}