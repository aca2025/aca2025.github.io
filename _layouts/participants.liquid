---
layout: default
---

{{ page.content }}

<br><br>

{% assign sorted_participants = site.data.participants | sort: 'last_name' %}

<table class="table table-striped">
  <tbody>
    {% for participant in sorted_participants %}   
      <tr>
        <td style="text-align: left"><strong><a href="{{ participant.homepage }}">{{ participant.first_name }} {{ participant.last_name }} - {{ participant.org }} ({{ participant.country }})</a></strong></td>
      </tr>
    {% endfor %}
  </tbody>
</table>