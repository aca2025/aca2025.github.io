---
layout: default
---

{{ page.content }}

<br><br>

{% assign sorted_participants = site.data.participants | sort: 'last_name' | where_exp: "item", "item.fee=='paid' or item.fee=='waived'"  %}


<table class="table table-striped">
  <tbody>
    {% assign cou = 0 %}   
    {% for participant in sorted_participants %}   
      {% assign cou = i | plus:1 %}
       <tr>
          <td style="text-align: left"><strong><a href="{{ participant.homepage }}">{{ cou }}. {{ participant.first_name }} {{ participant.last_name }} - {{ participant.org }} ({{ participant.country }})</a></strong></td>
        </tr>
      
    {% endfor %}
  </tbody>
</table>